{
  "name": "angular2-overview",
  "version": "1.0.0",
  "description": "Presentation slides.",
  "main": "index.js",
  "scripts": {
    "start": "tsc && npm run css && concurrently \"npm run tsc:w\" \"npm run css:w\" \"npm run lite\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "css": "node scripts/build-css",
    "css:w": "node scripts/build-css --watch",
    "typings": "typings",
    "add-slide": "node scripts/add-slide"
  },
  "keywords": [
    "Angular2",
    "Presentation"
  ],
  "author": "Dave F. Baskin <dfbaskin@gmail.com> (http://dfbaskin.com/)",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.4.0",
    "normalize.css": "^4.1.1",
    "prismjs": "^1.5.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "bluebird": "^3.4.0",
    "chokidar": "^1.5.2",
    "concurrently": "^2.0.0",
    "glob": "^7.0.3",
    "lite-server": "^2.2.0",
    "lodash": "^4.13.1",
    "postcss": "^5.0.21",
    "postcss-scss": "^0.1.8",
    "precss": "^1.4.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
